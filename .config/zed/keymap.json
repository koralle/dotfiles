[
  {
    "context": "Workspace",
    "bindings": {
      "shift shift": "file_finder::Toggle"
    }
  },
  {
    "context": "Workspace",
    "bindings": {
      "cmd cmd": "terminal_panel::ToggleFocus"
    }
  },
  {
    "context": "Editor",
    "bindings": {
      "shift e": "workspace::ToggleRightDock"
    }
  },
  {
    "context": "Editor && vim_mode == normal && !VimWaiting && !menu",
    "bindings": {
      "K": "editor::Hover",
      "space r": "editor::Rename",
      "space o": "project_symbols::Toggle",
      "space e": "project_panel::ToggleFocus",
      "space t": "terminal_panel::ToggleFocus",
      "space x": "workspace::CloseAllDocks",
      "shift t": "workspace::ToggleBottomDock"
    }
  },
  {
    "context": "Editor && vim_mode == insert && !VimWaiting && !menu",
    "bindings": {
      "ctrl-j": null,
      "ctrl-k": null,
      "shift-space": "editor::ShowCompletions"
    }
  },
  {
    "context": "Editor && showing_completions",
    "bindings": {
      "ctrl-y": "editor::ConfirmCompletion",
      "ctrl-j": "editor::SelectNext",
      "ctrl-k": "editor::SelectPrevious"
    }
  },
  {
    "context": "ProjectPanel && not_editing",
    "bindings": {
      "h": "project_panel::CollapseSelectedEntry",
      "l": "project_panel::ExpandSelectedEntry",
      "j": "menu::SelectNext",
      "k": "menu::SelectPrevious",
      "o": "menu::Confirm",
      "r": "project_panel::Rename",
      "z c": "project_panel::CollapseSelectedEntry",
      "z o": "project_panel::ExpandSelectedEntry",
      "x": "project_panel::Cut",
      "c": "project_panel::Copy",
      "p": "project_panel::Paste",
      "d": "project_panel::Delete",
      "a": "project_panel::NewFile",
      "shift-a": "project_panel::NewDirectory",
      "shift e": "workspace::ToggleRightDock"
    }
  },
  {
    "context": "Terminal",
    "bindings": {
      "shift shift": "workspace::ToggleZoom",
      "shift t": "workspace::ToggleBottomDock",
      "shift e": "workspace::ToggleRightDock"
    }
  },
  {
    "context": "FileFinder",
    "bindings": {
      "ctrl-k": "vim::SelectPrevious",
      "ctrl-j": "vim::SelectNext"
    }
  }
]
